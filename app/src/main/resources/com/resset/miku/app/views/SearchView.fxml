<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import org.kordamp.ikonli.javafx.FontIcon?>
<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.resset.miku.app.views.SearchController">
   <children>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="250.0" styleClass="contentNavigation" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="title" prefWidth="211.0" text="Search">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="50.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" />
               </padding>
            </Label>
            <ToggleButton alignment="BOTTOM_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Artists">
               <graphic>
                  <FontIcon iconLiteral="fas-user" iconSize="20" />
               </graphic>
            </ToggleButton>
            <ToggleButton alignment="BOTTOM_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Albums">
               <graphic>
                  <FontIcon iconLiteral="fas-compact-disc" iconSize="20" />
               </graphic>
            </ToggleButton>
            <ToggleButton alignment="BOTTOM_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Playlists">
               <graphic>
                  <FontIcon iconLiteral="fas-play-circle" iconSize="20" />
               </graphic>
            </ToggleButton>
            <ToggleButton alignment="BOTTOM_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#searchTracksButton" text="Tracks">
               <graphic>
                  <FontIcon iconLiteral="fas-music" iconSize="20" />
               </graphic>
            </ToggleButton>
         </children>
      </VBox>
      <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
         <children>
            <ScrollPane fx:id="searchContent" fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="300.0" vbarPolicy="ALWAYS" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="40.0" />
         </children>
         <HBox.margin>
            <Insets left="20.0" top="20.0" />
         </HBox.margin>
      </AnchorPane>
   </children>
</HBox>
